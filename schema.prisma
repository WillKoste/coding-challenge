generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

datasource db {
  provider = "sqlite"
}

model user {
  id                   Int    @id @default(autoincrement())
  first_name           String
  last_name            String
  email                String @unique
  password             String

  whitelisted_location whitelisted_location[]
}

model whitelisted_location {
  id                   Int    @id @default(autoincrement())
  country_name String
  user_id Int
  is_valid Boolean @default(false)
  user user @relation(fields: [user_id], references: [id])
}